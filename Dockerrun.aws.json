{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "php"
    },
    {
      "name": "nginx"
    }
  ],
  "containerDefinitions": [
    {
      "name": "php",
      "image": "641201314091.dkr.ecr.eu-central-1.amazonaws.com/api-php-dev:latest",
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 9000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "641201314091.dkr.ecr.eu-central-1.amazonaws.com/nginx:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "php"
      ]
    }
  ]
}